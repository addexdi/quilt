{
  "name": "quilt",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/lemonmade/quilt.git",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "sk test",
    "lint": "sk lint",
    "type-check": "sk type-check",
    "build": "sk build",
    "prerelease": "yarn build",
    "g": "npx -p @babel/core -p @babel/node babel-node --extensions .ts,.tsx,.mjs,.js --presets @babel/preset-typescript,@babel/preset-env ./scripts/generate.ts",
    "release": "lerna publish && git push --tags"
  },
  "devDependencies": {
    "@sewing-kit/cli": "^0.0.46",
    "@sewing-kit/config": "^0.0.36",
    "@sewing-kit/eslint-plugin": "^0.0.13",
    "@sewing-kit/plugin-eslint": "^0.0.35",
    "@sewing-kit/plugin-javascript": "^0.0.52",
    "@sewing-kit/plugin-jest": "^0.0.48",
    "@sewing-kit/plugin-package-flexible-outputs": "^0.0.47",
    "@sewing-kit/plugin-react": "^0.0.43",
    "@sewing-kit/plugin-typescript": "^0.0.56",
    "lerna": "^3.20.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {},
  "resolutions": {
    "@types/vfile-message": "1.0.1"
  }
}
